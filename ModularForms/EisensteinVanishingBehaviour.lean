theorem eisensteinSeries_is_tsum_eisensteincoeff {k m : ‚Ñï} (hk : 3 ‚â§ (k : ‚Ñ§))
 (a : Fin 2 ‚Üí ZMod (1 : ‚Ñï+))(keven :  k = 2 * m) (mne0 : m ‚â† 0){œÑ : ‚Ñç}:
 eisensteinSeries_MF hk a œÑ = (‚àë' (n : ‚Ñï), eisensteincoeff' k keven mne0 n ‚Ä¢ ùï¢ 1 œÑ ^ n) := by
  rw [eisensteinSeries_MF_is hk a keven mne0]
  have :   ‚àë' (n : ‚Ñï), eisensteincoeff' k keven mne0 n ‚Ä¢ ùï¢ 1 ‚ÜëœÑ ^ n = eisensteincoeff' k keven mne0 0 +
  ‚àë' (n : ‚Ñï), eisensteincoeff' k keven mne0 (n + 1) ‚Ä¢ ùï¢ 1 ‚ÜëœÑ ^ (n + 1):= by
    simp_all only [Nat.cast_mul, Nat.cast_ofNat, smul_eq_mul]
    simp_rw [‚Üê keven]
    have : eisensteincoeff' k keven mne0 0 =
     ‚àë ¬´i¬ª ‚àà Finset.range 1, eisensteincoeff' k keven mne0 ¬´i¬ª := by
      simp only [Finset.range_one, Finset.sum_singleton]
    symm
    rw [this]
    have : ‚àë ¬´i¬ª ‚àà Finset.range 1, eisensteincoeff' k keven mne0 ¬´i¬ª =
    ‚àë ¬´i¬ª ‚àà Finset.range 1, eisensteincoeff' k keven mne0 ¬´i¬ª * ùï¢ 1 œÑ ^ ¬´i¬ª := by
      simp only [Finset.range_one,
      Finset.sum_singleton, pow_zero, mul_one]
    rw [this]
    have :  ‚àë' (n : ‚Ñï), eisensteincoeff' k keven mne0 (n + 1) * ùï¢ 1 ‚ÜëœÑ ^ (n +1)  =
     ‚àë' (¬´i¬ª : ‚Ñï), eisensteincoeff' k keven mne0 (¬´i¬ª + 1) * ùï¢ 1 ‚ÜëœÑ ^ (¬´i¬ª +1 )
     := by rfl
    rw [this]
    have summablehyp:  Summable (fun ¬´i¬ª => eisensteincoeff' k
    keven mne0 ¬´i¬ª * ùï¢ 1 ‚ÜëœÑ ^ ¬´i¬ª) := by sorry
    rw [Summable.sum_add_tsum_nat_add 1 summablehyp]
  rw [this, ourEisensteinSeries_normalised (by linarith) a keven mne0]
  rw [eisensteincoeff'_zero]
  congr ; rw [‚Üê smul_eq_mul,
  ‚Üê tsum_const_smul'' ((OurBernoulli m mne0)‚Åª¬π * (2 * (2 * œÄ * i) ^ (2 * m))
  * (2 * m - 1).factorial ^ (-(1 : ‚Ñ§)) )]
  congr
  ext l
  rw [eisensteincoeff'notzero k keven mne0 ]
  rw [smul_eq_mul] ; simp_rw [keven] ; unfold Periodic.qParam
  have : cexp (2 * ‚ÜëœÄ * i * (œÑ : ‚ÑÇ) / (1 : ‚Ñù)) ^ (l + 1) =
  cexp (2 * ‚ÜëœÄ * i * (l + 1) * ‚ÜëœÑ ) := by simp only [ofReal_one, div_one]; sorry
  rw [this] ; simp_rw [‚Üê smul_eq_mul _ (cexp (2 * ‚ÜëœÄ * i * ((l : ‚ÑÇ) + 1) * (œÑ : ‚ÑÇ)))] ;
  rw [Summable.tsum_smul_const _ (cexp (2 * ‚ÜëœÄ * i * ((l : ‚ÑÇ) + 1) * (œÑ : ‚ÑÇ)))]
  simp_rw [smul_eq_mul] ; symm ; rw [mul_assoc]; congr ; ext S ;
  have : (2 * m - 1 : ‚Ñï) = 2 * (m : ‚Ñ§) -1 := by omega
  rw [‚Üê this] ; congr
  ¬∑ sorry --summable
  ¬∑ subst keven
    simp_all only [Nat.cast_mul, Nat.cast_ofNat, smul_eq_mul, eisensteincoeff'_zero, ne_eq, Nat.add_eq_zero,
    one_ne_zero, and_false, not_false_eq_true, eisensteincoeff'notzero, OurBenoullidef, inv_div, ofReal_div,
    ofReal_natCast, ofReal_mul, ofReal_pow, ofReal_neg, ofReal_one, ofReal_ofNat, ofReal_ratCast, Int.reduceNeg,
    zpow_neg, zpow_one, Set.coe_setOf, Set.mem_setOf_eq]

