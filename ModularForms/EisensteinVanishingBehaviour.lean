noncomputable def eisensteincoeff (k : ‚Ñï) : ‚Ñï ‚Üí ‚ÑÇ :=
  fun n => if n = 0 then (- (2 * œÄ * i) ^ k * (bernoulli' k) / Nat.factorial k)
  else (2 * œÄ * i) ^ k * (k - 1).factorial ^ (-(1 : ‚Ñ§)) * ‚àë' (m : {s | s + 1 ‚à£ n }), ((m: ‚ÑÇ) + 1) ^ (k - 1)

noncomputable def eisensteincoeff' (k : ‚Ñï) {m : ‚Ñï} (keven : k = 2 * m) (mne0 : m ‚â† 0)
: ‚Ñï ‚Üí ‚ÑÇ :=
  fun n => if n = 0 then 2
  else (OurBernoulli m mne0)‚Åª¬π * (2 * (2 * œÄ * i) ^ k) * (k - 1).factorial ^ (-(1 : ‚Ñ§))
  * ‚àë' (m : {s | s + 1 ‚à£ n }), ((m: ‚ÑÇ) + 1) ^ (k - 1)

